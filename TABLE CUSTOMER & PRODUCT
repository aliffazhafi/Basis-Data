------------------------------------------------------------------------------Table Customer-----------------------------------------------------------------------------------

CREATE DATABASE xcust;

CREATE TABLE xcust (
  customerId INTEGER PRIMARY KEY,
  customerName TEXT,
  contactName TEXT,
  address TEXT,
  city TEXT,
  postalcode TEXT,  -- Ubah tipe data postalcode menjadi TEXT
  country TEXT
);


