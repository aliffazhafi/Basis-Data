------------------------------------------------------------------------------Table Product-----------------------------------------------------------------------------------

CREATE TABLE products (
  ProductID INTEGER,
  ProductName TEXT,
  SupplierID INTEGER,
  CategoryID INTEGER,
  Unit TEXT,
  Price REAL
);

